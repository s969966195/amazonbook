    <script type="text/javascript">
        P.when("jQuery").execute(function ($) {
            var form = $("#addToCart"),
                thirdParty = $("#soldByThirdParty"),
                args = {
                    a: form.find("[name=ASIN]").val(),
                    me: form.find("[name=merchantID]")
                        .val(),
                    olid: form.find("[name=offerListingID]").val()
                };
            
            $('#selectQuantity [name="quantity"]').change(
                function () {
                    args.qt = this.value;
                    
                    
                    $.get("/gp/product/du/quantity-sip-update.html",
                            args,
                            function (data) {
                                data = $(data);
                                thirdParty.find(".price3P").html(data.filter(".price").html());
                                thirdParty.find(".price3PBadge").html(data.filter(".priceBadge").html());
                                
                                
                                
                                  thirdParty.find(".price3P").removeClass("a-hidden");
                                  thirdParty.find(".shipping3P").removeClass("a-hidden");
                                
                            }
                    );
                }
            );
        });
    </script>



            

            
            
                
                
                    
                    














    
    
    







