                s.crossOrigin = 'anonymous';
                s.src = u2;
                d.getElementsByTagName("head")[0].appendChild(s);
            },
            lj2: function(u) {
                var d = document;
                var s = d.createElement('script');
                s.type = 'text/javascript';
                s.crossOrigin = 'anonymous';
                s.src = u;
                d.getElementsByTagName("head")[0].appendChild(s);
            },
            go: function(){
                sitbLodStart = new Date().getTime();
                jQuery('body').css('overflow', 'hidden');
                var jqw = jQuery(window);
                var h = jqw.height();
                var w = jqw.width();
                var st = jqw.scrollTop();

                jQuery('#vellumShade').css({ 'top' : st, 'height' : h, 'width' : w }).show();

                var vli = jQuery('#vellumLdgIco');
                var nl = (w/2)-(vli.width()/2);
                var nt = st + (h/2) - (vli.height()/2);
                vli.css({"left" : nl, "top" : nt }).show();
                setTimeout("Vellum.x()", 20000);
                Vellum.pl();
            },
            x: function(){
                jQuery('#vellumMsgTxt').html("尝试显示此书时出错。");
                jQuery('#vellumMsgHdr').html("服务器超时");
                jQuery('#vellumMsg').show();
                var reftagImage = new Image();
                reftagImage.src = '/gp/search-inside/reftag/ref=rdr_bar_jsto';
            },
            h: function() {
                jQuery('#vellumMsg').hide();
                jQuery('#vellumShade').hide();
                jQuery('#vellumLdgIco').hide();
                jQuery("body").css('overflow', 'auto');
            },
            cf: function(a) {
                return function() {v.mt = a; v.rg = Array.prototype.slice.call(arguments); v.go()}
            },
            c: function(a) {
                var v = Vellum;
                v.mt = 'c'; v.rg = [a]; v.pl();
            },
            asin: 'B0065AUHZC',
            detailPageType: 'print',
            onReaderOpen: function() {
                jQuery.ajax({
                    url: '/gp/search-inside/service-data?method=getDetailPageRefTags&asin=B0065AUHZC&isReaderURL=false',
                    success: function(result) {postReftags(result, true);}
                });
            }
        }

        var f = "opqr".split('');
        for (var i in f) {
            var v = Vellum;
            v[f[i]] = v.cf(f[i]);
        }
        SitbReader = {
            LightboxActions: {
                openReader: function(r) {Vellum.o('B0065AUHZC', r); return false},
                openReaderToRandomPage: function(r) {Vellum.r('B0065AUHZC', r); return false},
                openReaderToSearchResults: function(q,r) {Vellum.q('B0065AUHZC',q,r); return false},
                openReaderToPage: function(p,t,r) {Vellum.p('B0065AUHZC',p,t,r); return false}
            }
        }
    }


     jQuery.ajax({
       url: '/gp/search-inside/service-data?method=getDetailPageRefTags&asin=B0065AUHZC&isReaderURL=false',
